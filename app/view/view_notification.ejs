<% extend('./layout/main') %>
<%- include('./includes/sidebar') -%>
 <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header top_50">
       
      <h1>
        Notification
        <small></small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="/dashboard"><i class="fa fa-dashboard"></i> Adtentio</a></li>
        <li class="active">Notification</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content container-fluid">
        <div id="ses_msg">
           <% if ( sessionFlash && sessionFlash.message) { %>
            
                <div class="alert alert-<%= sessionFlash.type %>">
                    <strong>FLASH!</strong> <%= sessionFlash.message %>
                </div>
            
            <% } %>
        </div>
        <div class="row pad-top side-cont" id="contact">
            <h3>
                <a class="btn btn-info pull-right" href="/contact">Back</a>
            </h3>
            <h2><%= contact.subject %></h2>
            <p><%= contact.create.toISOString().slice(0,10); %> | <%= contact.create.toISOString().slice(11,19); %>
            </p><hr>
            <p><%= contact.name %></p>
            <p>From: <%= contact.email %></p>
            <P>Mobile: <%= contact.mobile %></p>
            <div id="query" class="back_white col-lg-10 col-sm-10 col-md-10 col-xs-12 col-lg-offset-1 col-md-offset-1 col-sm-offset-1">
                <strong><%= contact.query %></strong>
            </div>
        </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
